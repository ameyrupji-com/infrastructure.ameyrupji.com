<div class="container-full-bg" id="image-back-main">
    <div class="jumbotron" id="gradient-main">
        <nav class="navbar navbar-expand-md navbar-light bg-transparent fixed-top">
            <div class="container">
                <a class="navbar-brand" href="#image-back-main"><img src="../src/images/ameyrupji-com-favicon-64x64.png" width="32" height="32" style="border-radius: 25%;margin-right: 10px;"><span id="navbar-brand-site-name">{{cover.site-name}}</span></a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbar-collapse-menu">
                    <div id="burger">
                        <div class="bar top"></div>
                        <div class="bar bottom"></div>
                    </div>
                </button>
                <div class="collapse navbar-collapse" id="navbar-collapse-menu">
                    <ul class="navbar-nav ml-auto">
                        {{#each cover.links}}
                        <li class="nav-item">
                            <a href="{{this.href}}" class="nav-link" data-toggle="collapse" data-target="#navbar-collapse-menu">{{this.text}}</a>
                        </li>
                        {{/each}}
                    </ul>
                    <hr/ class="d-block d-md-none d-lg-none d-xl-none">
                    <ul class="navbar-nav ml-auto d-block d-md-none d-lg-none d-xl-none">
                        {{#each app.other-links}}
                        <li class="nav-item">
                            <a href="{{this.href}}" class="nav-link" data-toggle="collapse" data-target="#navbar-collapse-menu">
                                {{this.text}}
                                {{#if this.img-src}}
                                    <img src="{{this.img-src}}">
                                {{/if}} &nbsp;
                                <i class="fas fa-external-link-alt"></i>
                            </a>
                        </li>
                        {{/each}}
                    </ul>
                    <hr/ class="d-block d-md-none d-lg-none d-xl-none">
                    <ul class="navbar-nav ml-auto d-block d-md-none d-lg-none d-xl-none">
                        <li class="nav-item">
                            <a href="{{this.href}}" class="nav-link" data-toggle="collapse" data-target="#navbar-collapse-menu">
                                Version: <span id="release-number-span" style="text-transform: lowercase;">{{app.active-version}}</span>
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
        <div id="jumbotron-content">
            <div class="container">
                <div class="row">
                    <div class="col-md-6" data-aos="fade-up" data-aos-anchor-placement="top-center" id="powered-by-col">
                        <div id="powered-by">
                            <h1 class="title"><span class="title"></span>Powered By</span> </h1>
                            <i class="fab fa-aws powered-by"></i>
                            <i class="circle-ci powered-by" style="content: url('https://api.iconify.design/simple-icons:circleci.svg?height=50&color=white');
    vertical-align: -0.125em;"></i>
                            <div id="website-btns">
                                <a class="btn btn-outline-light" href="{{app.website_link}}" role="button"><img src="../src/images/ameyrupji-com-favicon-64x64.png" width="20" height="20" style="border-radius: 25%; margin-right: 10px;"> Visit Website</a>
                                <a class="btn btn-outline-light" href="{{app.code_link}}" role="button"><i class="fab fa-github"></i> View Code</a>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6" data-aos="fade-left" data-aos-anchor-placement="top-center" id="infra-blueprint-col">
                        <div id="infra-blueprint" ></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="scroll-container">
        <div class="mouse">
            <div class="wheel">
            </div>
        </div>
        <div>
            <a href="{{cover.scroll-link}}">
                <span class="unu"></span>
                <span class="doi"></span>
                <span class="trei"></span>
            </a>
        </div>
    </div>
</div>