<div class="container-full-bg" id="image-back-main">
    <div class="jumbotron" id="gradient-main">
        <nav class="navbar navbar-expand-sm navbar-light bg-transparent fixed-top">
            <div class="container">
                <a class="navbar-brand" href="#summary-main">{{cover.first-name}} <span id="navbar-brand-alternate">{{cover.last-name}}</span> <span id="site-name">{{cover.site-name}}</span></a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbar-collapse-menu">
                    <div id="burger">
                        <div class="bar top"></div>
                        <div class="bar bottom"></div>
                    </div>
                </button>
                <div class="collapse navbar-collapse" id="navbar-collapse-menu">
                    <ul class="navbar-nav ml-auto">
                        {{#each cover.links}}
                        <li class="nav-item">
                            <a href="{{this.href}}" class="nav-link" data-toggle="collapse" data-target="#navbar-collapse-menu">{{this.text}}</a>
                        </li>
                        {{/each}}
                    </ul>
                    <hr/ class="d-block d-md-none d-lg-none d-xl-none">
                    <ul class="navbar-nav ml-auto d-block d-md-none d-lg-none d-xl-none">
                        {{#each app.other-links}}
                        <li class="nav-item">
                            <a href="{{this.href}}" class="nav-link" data-toggle="collapse" data-target="#navbar-collapse-menu">
                                {{this.text}}
                                {{#if this.img-src}}
                                    <img src="{{this.img-src}}">
                                {{/if}} &nbsp;
                                <i class="fas fa-external-link-alt"></i>
                            </a>
                        </li>
                        {{/each}}
                    </ul>
                    <hr/ class="d-block d-md-none d-lg-none d-xl-none">
                    <ul class="navbar-nav ml-auto d-block d-md-none d-lg-none d-xl-none">
                        <li class="nav-item">
                            <a href="{{this.href}}" class="nav-link" data-toggle="collapse" data-target="#navbar-collapse-menu">
                                Version: <span id="release-number-span" style="text-transform: lowercase;">{{app.app_version}}</span>
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
        <div id="jumbotron-content">
            <div class="container">
                <div class="row">
                    <div id="powered-by">
                        <h1 class="title"><span class="title"></span>Powered By</span> </h1>
                        <i class="fab fa-aws"></i>
                        <i class="circle-ci" style="content: url('https://api.iconify.design/simple-icons:circleci.svg?height=50&color=white');
vertical-align: -0.125em;"></i>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="scroll-container">
        <div class="mouse">
            <div class="wheel">
            </div>
        </div>
        <div>
            <a href="{{cover.scroll-link}}">
                <span class="unu"></span>
                <span class="doi"></span>
                <span class="trei"></span>
            </a>
        </div>
    </div>
</div>